{
  "name": "megii-app",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "17.8.0"
  },
  "private": true,
  "dependencies": {
    "@fancyapps/ui": "^4.0.26",
    "@popperjs/core": "^2.11.5",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "jquery-ui-dist": "^1.13.1",
    "mediaelement": "^5.0.5",
    "owl.carousel": "^2.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "node-sass": "^7.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:sass": "node-sass public/scss/style.scss public/css/style.css -w",
    "compile:sass": "node-sass public/scss/style.scss public/css/style.css",
    "concat:css": "concat -o public/css/style.concat.css fonts/flaticon/font/flaticon.css public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.concat.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css",
    "compile:bootsass": "node-sass public/scss/bootstrap/bootstrap.scss css/bootstrap/bootstrap.css",
    "mini:boot": "minify css/bootstrap/bootstrap.css > css/bootstrap.min.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.2.0"
}
